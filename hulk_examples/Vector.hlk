type Vector(collection, len){
    collection = collection;
    len = len
    current = -1;
    start = false

    function Next(){
        if (current <= len){
            self.start = true;
            self.current := current + 1;
            self.collection[current];
        }
        else self.start := false;
    }
    Current() => if (self.start) self.collection[current] else false;

    Reset() {
        self.start = false;
        self.current = -1;
    }
}